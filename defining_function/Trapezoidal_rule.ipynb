   "source": [
    "def Trapezoidal(a, b, n):\n",
    "    h = (b-a)/n\n",
    "    s = 0\n",
    "    for i in range(1,n+1):\n",
    "        s += h*(f(a+(i-1)*h) + f(a + i*h))/2\n",
    "        \n",
    "    return s"
   ]
